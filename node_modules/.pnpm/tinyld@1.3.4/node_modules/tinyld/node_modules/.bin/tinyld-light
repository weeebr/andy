#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/bin/node_modules:/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/node_modules:/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/tinyld@1.3.4/node_modules:/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/bin/node_modules:/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/tinyld@1.3.4/node_modules/tinyld/node_modules:/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/tinyld@1.3.4/node_modules:/Users/wea/dev/_private/andy/polyspeak/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tinyld-light.js" "$@"
else
  exec node  "$basedir/../../bin/tinyld-light.js" "$@"
fi
