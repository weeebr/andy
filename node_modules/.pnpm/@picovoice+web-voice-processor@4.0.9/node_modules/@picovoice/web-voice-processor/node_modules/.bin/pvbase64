#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules/@picovoice/web-utils/scripts/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules/@picovoice/web-utils/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules/@picovoice/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules/@picovoice/web-utils/scripts/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules/@picovoice/web-utils/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules/@picovoice/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/@picovoice+web-utils@1.3.1/node_modules:/Users/wea/dev/_private/andy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@picovoice+web-utils@1.3.1/node_modules/@picovoice/web-utils/scripts/base64.js" "$@"
else
  exec node  "$basedir/../../../../../../@picovoice+web-utils@1.3.1/node_modules/@picovoice/web-utils/scripts/base64.js" "$@"
fi
